import java.io.IOException;
import java.util.Scanner;
public class Main {
 
    public static void main(String[] args) throws IOException {
        Scanner read = new Scanner(System.in);
        
        int n = read.nextInt();
        
        for (int i=2;i<=n;i=i+2){
            int result = i*i;
            System.out.println(i+"^2 = "+result);
        }
 
    }
 
}
