import java.io.IOException;
import java.util.Scanner; 
public class Main {
 
    public static void main(String[] args) throws IOException {
 
        Scanner read = new Scanner(System.in);
        
        int codigo, quantidade;
        codigo = read.nextInt();
        quantidade = read.nextInt();
        
        if(codigo == 1){
            double totalhotdog = quantidade * 4.00;
            System.out.printf("Total: R$ %.2f\n", totalhotdog);
        }else if (codigo == 2){
            double totalsalada = quantidade * 4.50;
            System.out.printf("Total: R$ %.2f\n", totalsalada);
        }else if (codigo == 3){
            double totalbacon = quantidade * 5.00;
            System.out.printf("Total: R$ %.2f\n", totalbacon);
        }else if(codigo == 4){
            double totaltorrada = quantidade*2.00;
            System.out.printf("Total: R$ %.2f\n", totaltorrada);
        }else if(codigo ==5){
            double totalrefrigerante = quantidade*1.50;
            System.out.printf("Total: R$ %.2f\n", totalrefrigerante);
        }
        read.close();
    }
}
