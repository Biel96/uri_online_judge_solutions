import java.io.IOException;
import java.util.Scanner; 


public class Main {
 
    public static void main(String[] args) throws IOException {
 
        Scanner read = new Scanner(System.in);
        
        int a, t;
        a = read.nextInt();
        t = 2*a;
        System.out.println(t+" minutos");
 
    }
 
}
