import java.io.IOException;
import java.util.Scanner;
public class Main {
 
    public static void main(String[] args) throws IOException {
 
        Scanner read = new Scanner(System.in);
        
        int n = read.nextInt();
        
        for(int i =0; i<10; i++){
            int value = n*(i+1);
            System.out.println((i+1)+" x "+ n+" = "+value);
        }
 
    }
 
}
