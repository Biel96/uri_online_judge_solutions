import java.io.IOException;
import java.util.Scanner;
public class Main {
 
    public static void main(String[] args) throws IOException {
 
    Scanner read = new Scanner(System.in);
    
    int n = read.nextInt();
    if(n%2 != 0){
        for(int i=1; i<=n; i+=2){
            System.out.println(i);
        }
    }else{
        for(int i=1; i<n; i+=2){
            System.out.println(i);
        }
    }
    }
}
 
    
 
